public class LopHocPhan {

    public LopHocPhan() {
    }
    
    private String maLHP;
    private String tenLHP;
    private String tenGV;
    private String thongtinLopHoc;
        
    public LopHocPhan(String maLHP, String tenLHP, String tenGV, String thongtinLopHoc) {
        this.maLHP = maLHP;
        this.tenLHP = tenLHP;
        this.tenGV = tenGV;
        this.thongtinLopHoc = thongtinLopHoc;
    }

    @Override
    public String toString() {
        return "LopHocPhan{" + "maLHP=" + maLHP + ", tenLHP=" + tenLHP + ", tenGV=" + tenGV + ", thongtinLopHoc=" + thongtinLopHoc + '}';
    }

    public String getMaLHP() {
        return maLHP;
    }

    public void setMaLHP(String maLHP) {
        this.maLHP = maLHP;
    }

    public String getTenLHP() {
        return tenLHP;
    }

    public void setTenLHP(String tenLHP) {
        this.tenLHP = tenLHP;
    }

    public String getTenGV() {
        return tenGV;
    }

    public void setTenGV(String tenGV) {
        this.tenGV = tenGV;
    }

    public String getThongtinLopHoc() {
        return thongtinLopHoc;
    }

    public void setThongtinLopHoc(String thongtinLopHoc) {
        this.thongtinLopHoc = thongtinLopHoc;
    }
 
    
}
public class SinhVien {
    private String maSV;
    private String hoTen;

    @Override
    public String toString() {
        return "DS_SinhVien{" + "maSV=" + maSV + ", hoTen=" + hoTen + '}';
    }

    /**
     *
     * @param maSV
     * @param hoTen
     */
    public SinhVien(String maSV, String hoTen) {
        this.maSV = maSV;
        this.hoTen = hoTen;
    }

    public String getMaSV() {
        return maSV;
    }

    public void setMaSV(String maSV) {
        this.maSV = maSV;
    }

    public String getHoTen() {
        return hoTen;
    }

    public void setHoTen(String hoTen) {
        this.hoTen = hoTen;
    }
    
    
}
